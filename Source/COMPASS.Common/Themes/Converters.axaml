<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:conv="using:COMPASS.Common.Converters">
  <conv:MultiParamConverter x:Key="MultiParamConverter"/>
  <conv:AreEqualConverter Invert="False" x:Key="AreEqualConverter"/>
  <conv:AreEqualConverter Invert="True" x:Key="AreNotEqualConverter"/>
  <conv:SeverityToColorConverter x:Key="SeverityToColorConverter"/>
  <conv:IsSmallerOrEqualConverter x:Key="IsSmallerOrEqualConverter" />
  <conv:PlusOneConverter x:Key="PlusOneConverter" />
</ResourceDictionary>
