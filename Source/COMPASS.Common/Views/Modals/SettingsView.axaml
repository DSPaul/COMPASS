            <Setter Property="CornerRadius" Value="5" />
            <Setter Property="Margin" Value="5" />
            <Setter Property="FontSize" Value="16" />
            <Setter Property="Padding" Value="0,0,8,0" />
        </Style>
        <Style Selector="TabControl /template/ ContentPresenter">
            <Setter Property="CornerRadius" Value="0 10 10 0" />
            <Setter Property="Padding" Value="15" />
            <Setter Property="Background" Value="{StaticResource WindowBackground}"/>
        </Style>
        <Style Selector="TabControl /template/ ItemsPresenter#PART_ItemsPresenter > WrapPanel">
            <Setter Property="Margin" Value="0 10" />
            <Setter Property="ItemWidth" Value="150" />
        </Style>
        <!-- _________________________________________________ -->

        <Style Selector="TextBlock">
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>
        <Style Selector="TextBlock.SectionTitle">
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Foreground" Value="{StaticResource TextMutedColor}" />
            <Setter Property="FontWeight" Value="Light" />
            <Setter Property="FontSize" Value="20" />
        </Style>
        <Style Selector="Rectangle.SeparatorStyle">
            <Setter Property="Fill" Value="{StaticResource DarkUIElementBackground}" />
            <Setter Property="Width" Value="10000" />
            <Setter Property="Height" Value="3" />
            <Setter Property="Margin" Value="0,10" />
        </Style>
    </UserControl.Styles>
    <TabControl TabStripPlacement="Left"
                SelectedIndex="4"
                BorderThickness="0"
                Padding="0"
                Background="{StaticResource Layer2}"
                BorderBrush="{StaticResource Layer2}">
        <TabControl.Styles>
            <Style Selector="Border.SettingsContainer">
                <Setter Property="BorderThickness" Value="2" />
                <Setter Property="CornerRadius" Value="5" />
                <Setter Property="Padding" Value="20" />
                <Setter Property="BorderBrush" Value="{StaticResource DarkUIElementBackground}" />
            </Style>
        </TabControl.Styles>
        <TabControl.Resources>
            <x:Double x:Key="TabItemMinHeight"> 48 </x:Double>
        </TabControl.Resources>
        <TabItem Header="General">
            <StackPanel Orientation="Vertical" Spacing="20">
                <Border x:Name="Behaviour" Classes="SettingsContainer">
                    <StackPanel>
                        <TextBlock Classes="SectionTitle" Text="Behaviour" />
                        <Grid ColumnDefinitions="* Auto" 
                              RowDefinitions="Auto, Auto" RowSpacing="10">
                            <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal">
                                <TextBlock Text="On double click:" TextWrapping="Wrap" />
                            </StackPanel>
                            <Border Grid.Column="1" Grid.Row="0" Classes="RadioBtnStrip">
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <RadioButton GroupName="OpenPref"
                                                 Content="Prefer URL"
                                                 IsChecked="{Binding PreferOnlineSource}"
                                                 Background="{StaticResource AccentColorMuted}" />
                                    <RadioButton GroupName="OpenPref"
                                                 Content="Prefer local file"
                                                 IsChecked="{Binding !PreferOnlineSource, Mode=OneWay}"
                                                 Background="{StaticResource AccentColorMuted}" />
                                </StackPanel>
                            </Border>
                            
                            <StackPanel Grid.Column="0" Grid.Row="1" Orientation="Horizontal" Spacing="5">
                                <TextBlock Text="Treat tags and folders with the same name as linked"/>
                                <controls:Explainer Content="When a folder is linked to a tag, all future files that are imported from that folder will automatically be assigned that tag."/>
                            </StackPanel>
                            <CheckBox Grid.Column="1" Grid.Row="1" Classes="Toggle"
                                      HorizontalAlignment="Right"
                                      IsChecked="{Binding AutoLinkFolderTagSameName}"/>
                        </Grid>
                    </StackPanel>
                </Border>
                
                <Border x:Name="ManageLocalData" Classes="SettingsContainer">
                    <StackPanel>
                        <TextBlock Text="Manage Local Data" Classes="SectionTitle" />
                        <TextBlock Text="User data location:" Margin="0,10,0,5"/>
                        <Grid  ColumnDefinitions="*,auto,auto, auto" ColumnSpacing="5">
                            <TextBox IsReadOnly="True" Grid.Column="0" TextWrapping="Wrap"
                                     Text="{Binding CompassDataPath, Mode=OneWay}"/>
                            <controls:IconButton Grid.Column="1" Theme="{StaticResource IconOnly}" 
                                                 ToolTip.Tip="Show in explorer..." Icon="FolderOpen"
                                                 Command="{Binding BrowseLocalFilesCommand}"/>
                            <controls:IconButton Grid.Column="2" Theme="{StaticResource IconOnly}" 
                                                 ToolTip.Tip="Change data path..." Icon="FolderEdit" 
                                                 Command="{Binding ChangeDataPathCommand}"/>
                            <controls:IconButton Grid.Column="3" Theme="{StaticResource IconOnly}"  
                                                 ToolTip.Tip="Restore to default..." Icon="FolderRefresh"
                                                 Command="{Binding ResetDataPathCommand}"/>
                        </Grid>
                    </StackPanel>
                </Border>
