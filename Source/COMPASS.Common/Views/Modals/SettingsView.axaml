                            <TextBlock Text="Author: Paul De Smul" HorizontalAlignment="Center"/>
                        </UniformGrid>
                    </Border>
                    <Border Classes="SettingsContainer">
                        <StackPanel Orientation="Vertical">
                            <TextBlock Text="Links" Classes="SectionTitle"/>
                            <UniformGrid Columns="2">
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <TextBlock VerticalAlignment="Center" Text="Source Code:"/>
                                    <HyperlinkButton NavigateUri="https://github.com/DSPaul/COMPASS" 
                                                     Content="github.com/DSPaul/COMPASS"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <TextBlock VerticalAlignment="Center" Text="Buy Me a Drink:"/>
                                    <HyperlinkButton NavigateUri="https://ko-fi.com/pauldesmul" 
                                                     Content="ko-fi.com/pauldesmul"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <TextBlock VerticalAlignment="Center" Text="Join the Discord:"/>
                                    <HyperlinkButton NavigateUri="https://discord.gg/HawGMJgS9Y" 
                                                     Content="discord.gg/HawGMJgS9Y"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <TextBlock VerticalAlignment="Center" Text="Join the Subreddit:"/>
                                    <HyperlinkButton NavigateUri="https://www.reddit.com/r/compassapp/" 
                                                     Content="reddit.com/r/compassapp"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <TextBlock VerticalAlignment="Center" Text="Project Website:"/>
                                    <HyperlinkButton NavigateUri="https://www.compassapp.info" 
                                                     Content="compassapp.info"/>
                                </StackPanel>
                            </UniformGrid>
                        </StackPanel>
                    </Border>
